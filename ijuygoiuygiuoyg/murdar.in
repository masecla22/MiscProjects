#include <iostream>
using namespace std;

int main()
{

long long n;
long long s;
cin>>n;
    long long par = n/2;
    long long impar=0;
    if(n%2 == 0)
    {
        impar = (n/2);
    }
    else
    {
        impar = n/2+1;
    }
